{
  "name": "vite-demo",
  "version": "0.0.0",
  "scripts": {
    "start": "yarn tsync && vite",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "fullstack": "yarn && yarn concurrently --prefix-colors cyan,magenta --kill-others -n backend,frontend \"cargo --color always watch -x run -i frontend\" \"yarn start\"",
    "tsync": "tsync -i ../backend -o ./src/types/rust.d.ts"
  },
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.3.2",
    "@vitejs/plugin-react": "^1.0.0",
    "concurrently": "^6.2.1",
    "typescript": "^4.3.2",
    "vite": "^2.6.4"
  }
}